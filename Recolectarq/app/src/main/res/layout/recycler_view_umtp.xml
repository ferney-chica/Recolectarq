<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayoutRecyclerUmtp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#E2E2E2"
    tools:layout_editor_absoluteY="89dp">


    <android.support.constraint.ConstraintLayout
        android:id="@+id/cardViewUmtp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:clickable="true"
        android:focusable="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <android.support.v7.widget.AppCompatImageView
            android:id="@+id/imageViewUmtp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="22dp"
            android:layout_marginRight="22dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/add_24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textViewEditarUmtpCodigoRotulo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.36" />


        <TextView
            android:id="@+id/textViewUmtpId"
            android:layout_width="0dp"
            android:layout_height="27dp"
            android:layout_marginStart="24dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="16dp"
            android:text="Umtp Id:"
            android:textSize="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_conversion_absoluteHeight="27dp"
            tools:layout_conversion_absoluteWidth="52dp" />


        <ImageButton
            android:id="@+id/imageButtonEliminarUmtp"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="128dp"
            app:layout_constraintStart_toEndOf="@+id/imageButtonUmtpElemetosDiagnosticos"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@android:drawable/ic_delete" />

        <ImageButton
            android:id="@+id/imageButtonEditarUmtp"
            android:layout_width="37dp"
            android:layout_height="39dp"
            android:layout_marginStart="468dp"
            android:layout_marginLeft="468dp"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@android:drawable/ic_menu_edit" />

        <ImageButton
            android:id="@+id/imageButtonUmtpElemetosDiagnosticos"
            android:layout_width="37dp"
            android:layout_height="39dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageButtonEditarUmtp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.991"
            app:srcCompat="@android:drawable/ic_menu_agenda" />

        <TextView
            android:id="@+id/textViewUmtpTipoRelieve"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="212dp"
            android:layout_marginLeft="212dp"
            android:layout_marginTop="60dp"
            android:text="Tipo Relieve:"
            app:layout_constraintStart_toEndOf="@+id/textViewUmtpGeoforma"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewUmtpVegetacion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="152dp"
            android:layout_marginLeft="152dp"
            android:layout_marginTop="8dp"
            android:text="VegetaciÃ³n:"
            app:layout_constraintStart_toEndOf="@+id/textViewUmtpDedicacionEntorno"
            app:layout_constraintTop_toBottomOf="@+id/textViewUmtpTipoRelieve" />

        <TextView
            android:id="@+id/textViewUmtpMunicipio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="8dp"
            android:text="Municipio:"
            app:layout_constraintStart_toEndOf="@+id/imageViewUmtp"
            app:layout_constraintTop_toBottomOf="@+id/textViewUmtpDedicacionEntorno" />

        <TextView
            android:id="@+id/textViewUmtpVereda"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="96dp"
            android:layout_marginLeft="96dp"
            android:layout_marginTop="8dp"
            android:text="Vereda:"
            app:layout_constraintStart_toEndOf="@+id/textViewUmtpMunicipio"
            app:layout_constraintTop_toBottomOf="@+id/textViewUmtpVegetacion" />

        <TextView
            android:id="@+id/textViewUmtpSector"
            android:layout_width="wrap_content"
            android:layout_height="16dp"
            android:layout_marginStart="76dp"
            android:layout_marginLeft="76dp"
            android:layout_marginTop="8dp"
            android:text="Sector:"
            app:layout_constraintStart_toEndOf="@+id/textViewUmtpVereda"
            app:layout_constraintTop_toBottomOf="@+id/textViewUmtpVegetacion" />

        <TextView
            android:id="@+id/textViewUmtpGeoforma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="60dp"
            android:text="Geoforma:"
            app:layout_constraintStart_toEndOf="@+id/imageViewUmtp"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewUmtpDedicacionEntorno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="8dp"
            android:text="Dedicacion Entorno:"
            app:layout_constraintStart_toEndOf="@+id/imageViewUmtp"
            app:layout_constraintTop_toBottomOf="@+id/textViewUmtpGeoforma" />


    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>